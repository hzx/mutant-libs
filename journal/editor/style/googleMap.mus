
namespace googleMap {


node {
  position: relative;
  fontSize: 16px;
  /* margin: 16px; */
  padding: 28px 8px 8px 8px;
  /* backgroundColor: #f7f7f7; */
  borderBottom: 1px dotted #38569a;
}

node_active {
  backgroundColor: #f0f0f0;
}

move {
  position: absolute;
  top: 0;
  right: 0;
  width: 24px;
  height: 24px;
  background: url(img/google_map_small.png) center center no-repeat transparent;
  cursor: pointer;
}

row {
  overflow: hidden;
  /* marginBottom: 8px; */
}

wrap {
}

label {
  fontSize: 10px;
  fontFamily: Ubuntu, sans-serif;
  color: #2c597a;
}

input {
  margin: 0;
  padding: 0;
  borderStyle: none;
  outlineStyle: none;
  overflow: hidden;
  width: 100%;
  height: 100%;
  fontSize: inherit;
  fontFamily: inherit;
  /* color: #585858; */
  backgroundColor: transparent;
  /* color: #5c1510; */
  color: #c63f0d;
}

latNode {
  float: left;
  cssFloat: left;
  width: 200px;
  marginRight: 4px;
  backgroundColor: #f5f5f5;
}

