
class Link extends container.Model {
  auto src = new container.String();
  auto alt = new container.String();

  void setField(int field, string value) {
    switch field {
      case 1:
        id.init(value);
        break;
      case LinkField.SRC:
        src.init(value);
        break;
      case LinkField.ALT:
        alt.init(value);
        break;
    }
  }

  string serialize() {
    return "";
  }

  void unserialize(object pices) {
  }
}


enum LinkField {
  SRC = 2,
  ALT,
}
