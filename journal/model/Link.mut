
class Link {
  static int SRC = 2;
  static int ALT = 3;

  auto id = new container.String();
  auto src = new container.String();
  auto alt = new container.String();

  string getId() {
    return id.value;
  }

  void setField(int name, string value) {
    switch name {
      case 1:
        id.init(value);
        break;
      case Link.SRC:
        src.init(value);
        break;
      case Link.ALT:
        alt.init(value);
        break;
    }
  }
}
