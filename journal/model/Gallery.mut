
class Gallery {
  static int IMAGES = 2;

  static int THUMB_WIDTH = 396;
  static int THUMB_HEIGHT = 224;

  auto id = new container.String();
  auto images = new container.String();

  string getId() {
    return id.value;
  }

  void setField(int name, string value) {
    switch name {
      case 1:
        id.init(value);
        break;
      case Gallery.IMAGES:
        images.init(value);
        break;
    }
  }
}
