
class Gallery extends container.Model {
  static int THUMB_WIDTH = 396;
  static int THUMB_HEIGHT = 224;

  auto images = new container.String();

  void setField(int name, string value) {
    switch name {
      case 1:
        id.init(value);
        break;
      case GalleryField.IMAGES:
        images.init(value);
        break;
    }
  }

  string serialize() {
    return "";
  }

  void unserialize(object pices) {
  }
}


enum GalleryField {
  IMAGES = 2,
}
