
class Image extends container.Model {
  auto filename = new container.String();

  void setField(int name, string value) {
    switch name {
      case 1:
        id.init(value);
        break;
      case ImageField.FILENAME:
        filename.init(value);
        break;
    }
  }

  string serialize() {
    return "";
  }

  void unserialize(object pices) {
  }
}


enum ImageField {
  FILENAME = 2,
}
