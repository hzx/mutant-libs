
class File extends container.Model {
  auto nodeType = NodeType.FILE;

  auto pageId = new container.String();
  auto filename = new container.String();

  void setField(string key, string value) {
    int field = window.parseInt(key);
    switch field {
      case container.ModelField.ID:
        id.init(value);
        break;
      case FileField.PAGE_ID:
        pageId.init(value);
        break;
      case FileField.FILENAME:
        filename.init(value);
        break;
    }
  }

  string serialize() {
    auto buf = [
      container.ModelField.ID, id.value,
      FileField.PAGE_ID, pageId.value,
      FileField.FILENAME, filename.value,
    ];
    return buf.join(container.DELIM_FIELD);
  }

  void unserialize(object pices) {
    for int i = 0; i < pices.length; i=i+2 {
      setField(pices[i], pices[i+1]);
    }
  }
}


enum FileField {
  PAGE_ID = 2, // container.ModelField.ID + 1,
  FILENAME,
}
