
class Video {
  static int CODE = 2;

  auto id = new container.String();
  auto code = new container.String();

  string getId() {
    return id.value;
  }

  void setField(int name, string value) {
    switch name {
      case 1:
        id.init(value);
        break;
      case Video.CODE:
        code.init(value);
        break;
    }
  }
}
