
class String extends Event { // TODO: make own typed instead Event
  string value = "";
  bool has = false;

  void init(string value_) {
    value = value_;
    has = true;
  }

  void set(string value_) {
    has = true;
    if value isnot value_ {
      string old = value;
      value = value_;
      notify2(value, old);
    }
  }
}
