
interface Tagable {
  void create();
  void destroy();
  void enter();
  void exit();
  string getId();
  object getNode();
  tag getChild(string id);
  void insert(Tagable child);
  void insertBefore(Tagable child, string beforeHash);
  void append(Tagable child);
  void remove();
  void removeChild(string hash);
  void setValue(text);
}
